{
  "name": "kamihime-database",
  "version": "2.0.0",
  "description": "Website + API for Kamihime Database (Eros)",
  "main": "./build/index.js",
  "typings": "./src/typings",
  "scripts": {
    "build": "npm run test && npm run finalize",
    "test": "npm run lint && npm run compile",
    "compile": "tsc -p .",
    "lint": "npm run lint-src && npm run lint-static",
    "lint-fix": "npm run lint-src-fix && npm run lint-static-fix",
    "lint-src": "tslint -c .tslint.js -p .",
    "lint-src-fix": "npm run lint-src -- --fix",
    "lint-static": "tslint -c .tslint.js -p static/tsconfig.json",
    "lint-static-fix": "npm run lint-static -- --fix",
    "finalize": "node tools/finalize",
    "scenarios": "node build/util/scenarios",
    "scenarios-v": "npm run scenarios -- --verbose",
    "serve": "npm run build && node .",
    "pm2-serve": "npm run build && pm2 start .pm2.yml --env production"
  },
  "engine": {
    "node": ">=10"
  },
  "author": "gazmull",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gazmull/kamihime-database.git"
  },
  "bugs": {
    "url": "https://github.com/gazmull/kamihime-database/issues"
  },
  "homepage": "https://github.com/gazmull/kamihime-database#readme",
  "license": "MIT",
  "dependencies": {
    "@discordjs/uws": "10.149.0",
    "body-parser": "^1.18.3",
    "canvas": "^2.2.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "discord.js": "github:discordjs/discord.js",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "fuzzy": "^0.1.3",
    "infobox-parser": "^3.0.3",
    "knex": "^0.15.2",
    "mysql2": "^1.6.4",
    "node-fetch": "^2.3.0",
    "nodemw": "github:gazmull/nodemw#useTokenJustWhenNeeded",
    "pug": "^2.0.3",
    "serve-favicon": "^2.5.0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bootstrap": "^4.1.3",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/howler": "^2.0.5",
    "@types/jquery": "^3.3.23",
    "@types/js-cookie": "^2.2.0",
    "@types/knex": "^0.15.1",
    "@types/mysql2": "types/npm-mysql2",
    "@types/node": "^10.12.10",
    "@types/node-fetch": "^2.1.4",
    "@types/pug": "^2.0.4",
    "@types/serve-favicon": "^2.2.30",
    "@types/shortid": "^0.0.29",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.6",
    "uglifyjs-folder": "^1.5.1"
  }
}
